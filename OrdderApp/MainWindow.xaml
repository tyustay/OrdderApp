<Window x:Class="OrdderApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrdderApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#444549">
    <Grid>
        <Grid.RowDefinitions>
            
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Resources/6.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="OfficialTrade" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"> </TextBlock>
        <Grid Background="#506C69" Panel.ZIndex="-2" > </Grid>
        <Grid Grid.Row="2" Background="#506C69"></Grid>
        <Frame ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Back" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin="5" Click="BtnBack_Click" FontWeight="Bold" Background="#444549" Foreground="White"></Button>
        
    </Grid>
</Window>
